{
  "name": "prisma",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "prisma/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project prisma --web",
  "targets": {
    "seed": {
      "command": "ts-node seed/index.ts",
      "options": {
        "cwd": "prisma"
      }
    },
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "prisma"
      }
    },
    "migrate": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "prisma"
      }
    },
    "push": {
      "command": "prisma db push",
      "options": {
        "cwd": "prisma"
      }
    }
  }
}
